@model Dominio.Entidad.Negocio.Entidad.Proveedor

@{
    ViewBag.Title = "EliminarProveedor";
}



@if (TempData["mensaje"] != null)
{
    <div class="alert alert-success">@TempData["mensaje"]</div>
}
<h2>Eliminar Proveedor</h2>

<h3>Estas seguro de eliminarlo?</h3>
<div>
    <h4>Proveedor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.codigo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.codigo)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.codDistrito)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.codDistrito)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.fono)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fono)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar a Mantenimiento Porveedor", "MantLstProveedor")
        </div>
    }
</div>
